#!/usr/bin/env python
import sys, os, re

playlist = os.popen("mpc playlist")

if playlist.readlines()[0].find("AnimeNfo Radio") == -1:
    # Not in a stream :D
    song = os.popen("mpc").readlines()
    num = re.compile( r"#([0-9]*)/" )
    num = num.search(song[1]).groups()[0]
    os.system("mpc del "+str(num))
