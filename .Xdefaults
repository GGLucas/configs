! Pointer settings
Xcursor.size: 22
Xcursor.theme: Vanilla-DMZ-AA

! Font render settings
Xft*dpi:        99
Xft*antialias:    true

! urxvt settinsg
urxvt.termName: rxvt
urxvt.scrollBar: false
urxvt.scrollTtyOutput: false
urxvt.scrollWithBuffer: true
urxvt.scrollTtyKeypress: true
urxvt.secondaryScroll: true
urxvt.jumpScroll: true
urxvt.skipScroll: true
urxvt.perl-ext-common: default

urxvt.font: xft:Terminus:size=10:antialias=false:hinting=false
urxvt.boldFont: xft:Terminus:style=Bold:size=10:antialias=false:hinting=false

urxvt.background: #000000
urxvt.background: #000000
urxvt.foreground: #ffffff
urxvt.cursorColor: #FFFFFF
urxvt.cursorBlink: false

urxvt.saveLines: 500
urxvt.visualBell: false
urxvt.dynamicColors:  on
urxvt.highlightSelection:  true
urxvt.borderWidth: 0 
urxvt.urlLauncher: firefox
urxvt.pointerBlank: true
urxvt.internalBorder: 3 

! Colors
urxvt*color0:      #2E3436
urxvt*color1:      #CC0000
urxvt*color2:      #4E9A06
urxvt*color3:      #C4A000
urxvt*color5:      #744E7A
urxvt*color6:      #06989A
urxvt*color7:      #D3D7CF
urxvt*color8:      #555753
urxvt*color9:      #EF2929
urxvt*color10:     #8AE234
urxvt*color11:     #FCE94F
urxvt*color12:     #729FCF
urxvt*color13:     #AD7FA8
urxvt*color14:     #59BCD9
urxvt*color15:     #EEEEEC
